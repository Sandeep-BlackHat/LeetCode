class Solution {
public:
    int numIdenticalPairs(vector<int>& nums) {
        int count = 0;
        for(int i=0; i<nums.size(); i++){
            for(int j=i; j<nums.size(); j++){
                if(i < j){
                    if(nums[i] == nums[j]){
                        count++;
                    }
                }
                cout<<endl;
            }
        }
        return count;
    }
};
